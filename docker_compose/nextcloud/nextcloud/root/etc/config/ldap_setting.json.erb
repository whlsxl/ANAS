{
  "apps": {
    "user_ldap": {
      "types": "authentication",
      "s01ldap_port": <%= envs["SAMBA_DC_LDAPS_PORT"] %>,
      "s01ldap_dn": "<%= envs["SAMBA_DC_ADMINISTRATOR_DN"] %>",
      "s01ldap_agent_password": "***",
      "s01ldap_base": "<%= envs["SAMBA_DC_BASE_DN"] %>",
      "s01ldap_base_groups": "<%= envs["SAMBA_DC_BASE_GROUPS_ROLE_DN"] %>",
      "s01ldap_base_users": "<%= envs["SAMBA_DC_BASE_USERS_DN"] %>",
      "s01ldap_configuration_active": "1",
      "s01ldap_group_filter": "<%= envs["SAMBA_DC_GROUP_CLASS_FILTER"] %>",
      "s01ldap_groupfilter_objectclass": "<%= envs["SAMBA_DC_GROUP_CLASS_NAME"] %>",
      "s01ldap_host": "<%= envs["SAMBA_DC_LDAPS_SERVER_URL"] %>",
      "s01ldap_login_filter": "<%= envs["NEXTCLOUD_USER_LOGIN_FILTER"] %>",
      "s01ldap_userlist_filter": "<%= envs["NEXTCLOUD_USER_FILTER"] %>",
      "s01ldap_display_name": "<%= envs["SAMBA_DC_USER_DISPLAY_NAME"] %>",
      "s01ldap_group_display_name": "<%= envs["SAMBA_DC_GROUP_DISPLAY_NAME"] %>",
      "s01ldap_email_attr": "<%= envs["SAMBA_DC_USER_EMAIL"] %>",
      <% unless envs["NEXTCLOUD_DEFAULT_QUOTA"].nil? || envs["NEXTCLOUD_DEFAULT_QUOTA"].empty? %>
      "s01ldap_quota_def": "<%= envs["NEXTCLOUD_DEFAULT_QUOTA"] %>",
      <% end %>
      "s01ldap_nested_groups": 1,
      "s01ldap_expert_uuid_group_attr": "sAMAccountName",
      "s01ldap_expert_uuid_user_attr": "cn",
      "s01ldap_turn_on_pwd_change": 1
    }
  }
}